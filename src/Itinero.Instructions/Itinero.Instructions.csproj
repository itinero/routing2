<Project Sdk="Microsoft.NET.Sdk">
    <Import Project="../../Itinero.Common.props" />

    <PropertyGroup>
        <TargetFramework>netstandard2.1</TargetFramework>
        <AssemblyName>Itinero.Instructions</AssemblyName>
        <LangVersion>default</LangVersion>
        <PackageId>Itinero.Instructions</PackageId>
        <Nullable>enable</Nullable>
        <GenerateDocumentationFile>true</GenerateDocumentationFile>
    </PropertyGroup>

    <ItemGroup>
      <ProjectReference Include="..\Itinero\Itinero.csproj" />
    </ItemGroup>

    <ItemGroup>
      <None Remove="Languages\en-GB.json" />
      <EmbeddedResource Include="Languages\en-GB.json" />
    </ItemGroup>

</Project>
